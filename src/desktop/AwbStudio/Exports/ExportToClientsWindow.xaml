<Window x:Class="AwbStudio.Exports.ExportToClientsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AwbStudio.Exports"
        mc:Ignorable="d"
        Title="ExportToClientsWindow"  Height="450" Width="800">
    <Window.Icon>
        <DrawingImage />
    </Window.Icon>
    <Grid>
        <StackPanel>
            <Button x:Name="ButtonWriteToEsp32RemoteClient" Content="Write ESP32 remote client sourcecode" Click="ButtonWriteToEsp32RemoteClient_Click"  />
            <Label Content="Target folder:" />
            <Label x:Name="LabelTargetFolder"  Content="LabelTargetFolder" MinHeight="20"/>
        </StackPanel>
    </Grid>
</Window>
