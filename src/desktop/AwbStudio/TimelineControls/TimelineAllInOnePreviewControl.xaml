<UserControl x:Class="AwbStudio.TimelineControls.TimelineAllInOnePreviewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AwbStudio.TimelineControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid 
        PreviewMouseDown="AllValuesGrid_PreviewMouseDown" 
        PreviewMouseMove="AllValuesGrid_PreviewMouseMove"
        Background="Black">
        <Grid x:Name="OpticalGrid" IsHitTestVisible="False"></Grid>
        <Grid x:Name="AllValuesGrid"></Grid>
        <Grid x:Name="PlayPosGrid"></Grid>
    </Grid>
</UserControl>
