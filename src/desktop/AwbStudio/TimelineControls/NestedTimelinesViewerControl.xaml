<UserControl x:Class="AwbStudio.TimelineControls.NestedTimelinesViewerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AwbStudio.TimelineControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid PreviewMouseDown="Grid_PreviewMouseDown">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <local:ValueEditorHeaderControl x:Name="HeaderControl" Grid.Row="0"/>
        <Grid Grid.Row="1" Background="Transparent">
            <Grid x:Name="AllValuesGrid" >
            </Grid>
        </Grid>
    </Grid>
</UserControl>
